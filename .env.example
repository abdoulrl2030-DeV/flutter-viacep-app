# Variáveis de Ambiente - flutter-viacep-app
# IMPORTANTE: Não commitar este arquivo no repositório!
# Use: git update-index --assume-unchanged .env

# API Configuration
VIACEP_API_URL=https://viacep.com.br/ws
API_TIMEOUT=10000
MAX_REQUESTS_PER_MINUTE=10

# App Configuration
APP_NAME=ViaCEP App
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1
APP_PACKAGE_NAME=com.abdoulrl2030.viacep_app

# Android Configuration
ANDROID_KEYSTORE_PATH=android/app/keystore.jks
ANDROID_KEYSTORE_PASSWORD=changeme123!
ANDROID_KEY_ALIAS=viacep_key
ANDROID_KEY_PASSWORD=changeme123!

# iOS Configuration
IOS_CERTIFICATE_PATH=ios/certificates/distribution.p12
IOS_CERTIFICATE_PASSWORD=changeme123!
IOS_PROVISIONING_PROFILE_PATH=ios/provisioning/ViaCEPApp.mobileprovision
IOS_TEAM_ID=XXXXXXXXXX
IOS_BUNDLE_ID=com.abdoulrl2030.viacepApp

# Build Configuration
BUILD_RELEASE_APK=true
BUILD_RELEASE_AAB=true
BUILD_IOS_APP=false
BUILD_WEB_APP=true

# Deployment Configuration
DEPLOY_TO_PLAYSTORE=false
DEPLOY_TO_TESTFLIGHT=false
DEPLOY_TO_WEB=false
WEB_DEPLOY_URL=https://your-domain.com

# Analytics & Monitoring
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
LOG_LEVEL=INFO

# Security
ENFORCE_HTTPS=true
ENABLE_SSL_PINNING=true
ENABLE_OBFUSCATION=true
ENABLE_SHRINKING=true

# Staging/Production
ENVIRONMENT=development
# Values: development, staging, production
